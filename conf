<VirtualHost *:80>
		ServerName devupteam.ru
		ServerAdmin email@example.com
		WSGIScriptAlias / /var/www/dev/dev.wsgi
		<Directory /var/www/dev/>
			Order allow,deny
			Allow from all
		</Directory>
		Alias /static /var/www/dev/static
		<Directory /var/www/dev/static/>
			Order allow,deny
			Allow from all
		</Directory>
		ErrorLog ${APACHE_LOG_DIR}/error.log
		LogLevel warn
		CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>